{
    "manifest_version": 3,
    "name": "SBG Enhanced UI",
    "description": "Расширение для браузера, добавляющее новый функционал SBG Enhanced UI без установки дополнительных расширений",
    "version": "6.5.1",
    "permissions": ["unlimitedStorage", "scripting", "geolocation"],
    "content_scripts": [
        {
            "matches": ["https://sbg-game.ru/app", "https://sbg-game.ru/app/*"],
            "js": ["private.user.js"]
        }
    ],
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "64": "images/64.png",
        "128": "images/128.png"
    }
}