{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "ignorePatterns": ["/dist/index.js", "/index.js", "/webpack.config.js"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "sort-imports": ["error"]
  }
}
